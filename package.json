{
  "name": "ncodr",
  "version": "0.9.2",
  "description": "Encoding/Ripping Queue",
  "engines": [
    ">= 8.3.0"
  ],
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LEVEL=error nyc mocha --opts mocha.opts test/unit",
    "serve": "backpack dev | pino-clf -a 1 -d 2 | pino-pretty",
    "lint": "eslint .",
    "jsdoc": "jsdoc",
    "build": "rm -rf build && backpack build",
    "version": "node -e 'console.log(require(\"./package.json\").version)'",
    "docker": "docker build -t aztechian/ncodr:$(npm run -s version) .",
    "start": "node build/main.js | pino-clf -a 1 -d 2 | pino-pretty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aztechian/ncodr.git"
  },
  "keywords": [
    "encode",
    "queue",
    "handbrake",
    "avconv",
    "dvdbackup",
    "makemkv"
  ],
  "author": "Ian Martin <ian@imartin.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aztechian/ncodr/issues"
  },
  "homepage": "https://github.com/aztechian/ncodr#readme",
  "dependencies": {
    "atob": "^2.0.3",
    "babel-register": "^6.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "bull": "^3.4.4",
    "cheerio": "*",
    "chownr": "^1.0.1",
    "compression": "^1.7.2",
    "config": "^1.31.0",
    "express": "^4.16.3",
    "express-pino-logger": "^4.0.0",
    "got": "^9.2.2",
    "helmet": "^3.13.0",
    "js-yaml": "^3.12.0",
    "jsonwebtoken": "^8.3.0",
    "keyv": "^3.0.0",
    "ncodr-ui": "github:aztechian/ncodr-ui#v1.2.7",
    "parse-cache-control": "^1.0.1",
    "pino": "^5.6.2",
    "pino-pretty": "^2.2.0",
    "pino-clf": "^1.0.6",
    "source-map-support": "^0.5.6",
    "sse-express": "^1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-env": "^1.6.1",
    "backpack-core": "^0.7.0",
    "chai": "*",
    "chai-as-promised": "*",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-import-resolver-babel-root-import-fixed": "^1.0.3",
    "eslint-plugin-import": "^2.13.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mock-spawn": "*",
    "nock": "^9.5.0",
    "nyc": "^12.0.2",
    "sinon": "^6.1.4",
    "sinon-chai": "^3.2.0",
    "sinon-express-mock": "^2.0.5"
  }
}
