{
  "name": "ncodr",
  "version": "0.10.0",
  "description": "Encoding/Ripping Queue",
  "engines": {
    "node": ">= 8.3.0"
  },
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LEVEL=error nyc mocha --opts mocha.opts test/unit",
    "serve": "backpack dev | pino-clf -a 1 -d 2 | pino-pretty",
    "lint": "eslint .",
    "jsdoc": "jsdoc",
    "build": "rm -rf build && backpack build",
    "package": "npm prune --production && zip -r ncodr-$(npm run -s version).zip build config node_modules LICENSE package.json package-lock.json README",
    "version": "node -e 'console.log(require(\"./package.json\").version)'",
    "docker": "docker build -t aztechian/ncodr:$(npm run -s version) .",
    "start": "node build/main.js | pino-clf -a 1 -d 2 | pino-pretty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aztechian/ncodr.git"
  },
  "keywords": [
    "encode",
    "queue",
    "handbrake",
    "avconv",
    "dvdbackup",
    "makemkv"
  ],
  "author": "Ian Martin <ian@imartin.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aztechian/ncodr/issues"
  },
  "homepage": "https://github.com/aztechian/ncodr#readme",
  "dependencies": {
    "atob": "^2.1.2",
    "babel-register": "^6.0",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "bull": "^3.7.0",
    "cheerio": "*",
    "chownr": "^1.1.1",
    "compression": "^1.7.2",
    "config": "^3.0.1",
    "express": "^4.16.3",
    "express-pino-logger": "^4.0.0",
    "got": "^9.6.0",
    "helmet": "^3.15.0",
    "js-yaml": "^3.12.2",
    "jsonwebtoken": "^8.5.0",
    "keyv": "^3.1.0",
    "ncodr-ui": "github:aztechian/ncodr-ui#v1.3.1",
    "parse-cache-control": "^1.0.1",
    "pino": "^5.11.1",
    "pino-pretty": "^3.0.0",
    "pino-clf": "^1.0.6",
    "source-map-support": "^0.5.10",
    "sse-express": "^1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-env": "^1.7.0",
    "backpack-core": "^0.7.0",
    "chai": "*",
    "chai-as-promised": "*",
    "eslint": "^5.15.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.17.2",
    "jsdoc": "^3.5.5",
    "mocha": "^6.0.0",
    "mock-spawn": "*",
    "nock": "^10.0.6",
    "nyc": "^14.0.0",
    "sinon": "^7.2.6",
    "sinon-chai": "^3.3.0",
    "sinon-express-mock": "^2.1.0"
  }
}
